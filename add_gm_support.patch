+func Uint64ToAffine(in []uint64)(x, y *big.Int){
+	var r p256Point
+	for i:=0;i<12;i++{
+		r.xyz[i]=in[i]
+	}
+	tmpx,tmpy:=r.p256PointToAffine()
+	return tmpx,tmpy
+}
