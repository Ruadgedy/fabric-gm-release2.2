+	for _, suiteId := range possibleCipherSuites {
+		for _, suite := range config.GMSupport.cipherSuites() {
+			if suite.id != suiteId {
+				continue
+			}
+			hello.cipherSuites = append(hello.cipherSuites, suiteId)
+			continue NextCipherSuite
