+	MOVQ (8*1)(x_ptr), acc1
+	MOVQ (8*2)(x_ptr), acc2
+	MOVQ (8*3)(x_ptr), acc3
+	XORQ acc4, acc4
+
+	// Only reduce, no multiplications are needed
+	// First stage
+    MOVQ acc0, AX
+	MOVQ acc0, t1
+	//32
+	SHLQ $32, acc0
+	SHRQ $32, t1
+	ADDQ AX, acc1
+	ADCQ $0, acc2
+	SUBQ acc0, acc1
+    SBBQ t1, acc2
+    SBBQ $0, acc3
+    MULQ p256const2<>(SB)
+	ADDQ AX, acc3
+	ADCQ DX, acc4
+	XORQ acc5, acc5
+
