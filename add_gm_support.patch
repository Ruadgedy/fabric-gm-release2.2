+		vers:                         config.GMSupport.GetVersion(),
+		compressionMethods:           []uint8{compressionNone},
+		random:                       make([]byte, 32),
+	}
+	possibleCipherSuites := getCipherSuites(config)
+	hello.cipherSuites = make([]uint16, 0, len(possibleCipherSuites))
+
