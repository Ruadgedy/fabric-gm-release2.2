+	MOVQ acc1, t1
+	//32
+	SHLQ $32, acc1
+	SHRQ $32, t1
+	ADDQ AX, acc2
+	ADCQ $0, acc3
+    SUBQ acc1, acc2
+    SBBQ t1, acc3
+    SBBQ $0, acc4
+    MULQ p256const2<>(SB)
+	ADDQ AX, acc4
+	ADCQ DX, acc5
+	XORQ acc0, acc0
+
+	// Third stage
+	MOVQ acc2, AX
+	MOVQ acc2, t1
+	//32
+	SHLQ $32, acc2
+	SHRQ $32, t1
+	ADDQ AX, acc3
+	ADCQ $0, acc4
+    SUBQ acc2, acc3
+    SBBQ t1, acc4
+    SBBQ $0, acc5
+    MULQ p256const2<>(SB)
