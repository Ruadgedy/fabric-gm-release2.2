+		x11, y11 := c.ScalarMult(pub.X, pub.Y, t.Bytes())
+		x12, y12 := c.ScalarBaseMult(s.Bytes())
+		x1, _ = c.Add(x11, y11, x12, y12)
+	}
+
+	x := new(big.Int).Add(e, x1)
+	x = x.Mod(x, n)
+
+	return x.Cmp(r) == 0
+}
