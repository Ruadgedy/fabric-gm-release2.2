+    //32
+    SHLQ $32, acc3
+    SHRQ $32, t1
+    ADDQ AX, acc4
+    ADCQ $0, acc5
+    SUBQ acc3, acc4
+    SBBQ t1, acc5
+    SBBQ $0, acc0
+    MULQ p256const2<>(SB)
