+	SHLQ $32, acc0
+	SHRQ $32, t1
+	ADDQ AX, acc1
+	ADCQ $0, acc2
+	SUBQ acc0, acc1
+    SBBQ t1, acc2
+    SBBQ $0, acc3
+    MULQ p256const2<>(SB)
+	ADDQ AX, acc3
+	ADCQ DX, acc4
+	ADCQ $0, acc5
